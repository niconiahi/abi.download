{
  "private": true,
  "name": "abi.download",
  "description": "Here you can find and download your required ABI",
  "license": "MIT",
  "scripts": {
    "commit": "npx cz",
    "deploy": "npm run build && wrangler publish",
    "reinstall": "rm -rf node_modules && yarn",
    "postinstall": "remix setup cloudflare-workers",
    "build": "npm run build:css && npm run build:remix && npm run build:worker",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:remix": "remix build",
    "build:worker": "esbuild --define:process.env.NODE_ENV='\"production\"' --minify --bundle --sourcemap --outdir=dist ./worker",
    "dev": "concurrently \"npm run dev:remix\" \"npm run dev:miniflare\" \"npm run dev:css\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "dev:remix": "remix watch",
    "dev:worker": "esbuild --define:process.env.NODE_ENV='\"development\"' --bundle --sourcemap --outdir=dist ./worker",
    "dev:miniflare": "miniflare --build-command \"npm run dev:worker\" --watch"
  },
  "dependencies": {
    "@remix-run/cloudflare-workers": "^1.0.6",
    "@remix-run/react": "^1.0.6",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@cloudflare/wrangler": "^1.19.5",
    "@remix-run/dev": "^0.21.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "autoprefixer": "^10.4.0",
    "commitizen": "^4.2.4",
    "concurrently": "^6.4.0",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "^0.13.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "miniflare": "2.0.0-next.3",
    "postcss": "^8.4.1",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js",
  "version": "0.5.1"
}
